{
  "name": "snippet-generator",
  "displayName": "Snippet Generator",
  "version": "0.1.0",
  "publisher": "fiore57",
  "description": "Simple snippet generator for VSCode",
  "author": {
    "name": "fiore57"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.generateSnippet"
  ],
  "engines": {
    "vscode": "^1.39.0"
  },
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateSnippet",
        "title": "%cmd.generateSnippet.title%"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.generateSnippet",
          "group": "9_cutcopypaste@100"
        }
      ]
    },
    "keybindings": {
      "command": "extension.generateSnippet",
      "key": "ctrl+alt+s"
    },
    "configuration": {
      "title": "generateSnippet",
      "properties": {
        "generateSnippet.snippetFilePath": {
          "type": [
            "string",
            "null"
          ],
          "default": "default",
          "description": "%cfg.snippetFilePath.description%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/src/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.4",
    "@types/vscode": "^1.44.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "istanbul": "^0.4.5",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/fiore57/snippet-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fiore57/snippet-generator.git"
  },
  "homepage": "https://github.com/fiore57/snippet-generator/blob/master/README.md"
}
